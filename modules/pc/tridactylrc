bind d scrollpage -1
bind d scrollpage 0.5
bind e scrollpage -0.5
bind O fillcmdline tabopen
bind q tabclose
bind s scrollline 10
bind S back
bind D forward
bind w scrollline -10

bind <Space> scrollpage 1

